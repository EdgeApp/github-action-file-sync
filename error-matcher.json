{
    "problemMatcher": [
        {
            "owner": "genericError",
            "pattern": [
                {
                    "regexp": "^(.*error: .*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        },
        {
            "owner": "gitRemoteRejectedError",
            "pattern": [
                {
                    "regexp": "^.*(\\[remote rejected\\] .*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        },
        {
            "owner": "copyError",
            "pattern": [
                {
                    "regexp": "^.*cp: can't stat .*$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        },
        {
            "owner": "gitRepoError",
            "pattern": [
                {
                    "regexp": "^.*fatal: not a git repository .*$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        }
    ]
}
